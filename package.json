{
  "name": "1stopui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:main": "vite --mode development",
    "dev:ess": "vite --mode development --config vite.config.ess.ts",
    "dev:yourstop": "cd src/yourstop/frontend && npm run dev",
    "dev:all": "concurrently \"npm run dev:main\" \"npm run dev:yourstop\" --names \"MAIN,YOURSTOP\" --prefix-colors \"blue,green\"",
    "build": "vite build",
    "build:main": "vite build --mode production",
    "build:ess": "vite build --mode production --config vite.config.ess.ts",
    "build:yourstop": "cd src/yourstop/frontend && npm run build",
    "build:all": "npm run build:main && npm run build:yourstop",
    "build:check": "tsc && vite build",
    "build:check:main": "tsc && npm run build:main",
    "build:check:ess": "tsc && npm run build:ess",
    "build:check:yourstop": "cd src/yourstop/frontend && npm run typecheck && npm run build",
    "build:check:all": "npm run build:check:main && npm run build:check:yourstop",
    "preview": "vite preview",
    "preview:main": "vite preview --mode production",
    "preview:ess": "vite preview --mode production --config vite.config.ess.ts",
    "preview:yourstop": "cd src/yourstop/frontend && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:main": "npm run lint && npm run build:check:main",
    "test:ess": "npm run lint && npm run build:check:ess",
    "test:yourstop": "cd src/yourstop/frontend && npm run typecheck",
    "test:all": "npm run test:main && npm run test:yourstop",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "deploy:main": "npm run build:main && echo \"Build complete. Deploy dist/ folder to your hosting service.\"",
    "deploy:ess": "npm run build:ess && echo \"Build complete. Deploy dist-ess/ folder to your hosting service.\"",
    "deploy:yourstop": "npm run build:yourstop && echo \"Build complete. Deploy .next/ or out/ folder to your hosting service.\"",
    "deploy:all": "npm run build:all && echo \"All builds complete. Deploy dist/ (main) and .next/ or out/ (YourStop) folders.\"",
    "clean": "node scripts/clean.cjs dist node_modules/.vite",
    "clean:ess": "node scripts/clean.cjs dist-ess",
    "clean:yourstop": "node scripts/clean.cjs src/yourstop/frontend/.next src/yourstop/frontend/out src/yourstop/frontend/node_modules/.cache",
    "clean:all": "npm run clean && npm run clean:ess && npm run clean:yourstop",
    "install:all": "npm install && cd src/yourstop/frontend && npm install"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emailjs/browser": "^4.4.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@firebase/vertexai": "^1.2.4",
    "@google-cloud/vertexai": "^1.10.0",
    "@iconify/react": "^6.0.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@mui/icons-material": "^5.17.1",
    "@mui/lab": "^5.0.0-alpha.125",
    "@mui/material": "^5.14.19",
    "@mui/x-data-grid": "^7.28.3",
    "@mui/x-date-pickers": "^7.28.3",
    "@react-spring/web": "^10.0.0",
    "@types/jspdf": "^1.3.3",
    "@types/papaparse": "^5.3.16",
    "@types/pdf-parse": "^1.1.5",
    "@types/react-beautiful-dnd": "^13.1.8",
    "axios": "^1.9.0",
    "chart.js": "^4.4.8",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.13",
    "emoji-picker-react": "^4.12.2",
    "firebase": "^12.1.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.562.0",
    "nodemailer": "^7.0.10",
    "papaparse": "^5.5.3",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^4.8.69",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-animated-weather": "^4.0.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-color": "^2.19.3",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.5.0",
    "react-firebase-hooks": "^5.1.1",
    "react-redux": "^9.2.0",
    "react-resizable": "^3.0.5",
    "react-rnd": "^10.5.2",
    "react-router-dom": "^6.20.0",
    "recharts": "^2.15.2",
    "sonner": "^2.0.7",
    "tesseract.js": "^6.0.1",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.41",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.37",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^18.2.15",
    "@types/react-redux": "^7.1.34",
    "@types/react-resizable": "^3.0.8",
    "@types/react-rnd": "^8.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
